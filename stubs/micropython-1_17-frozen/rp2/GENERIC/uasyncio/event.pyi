import uio
from . import core as core
from typing import Any

class Event:
    state: bool
    waiting: Any
    def __init__(self) -> None: ...
    def is_set(self): ...
    def set(self) -> None: ...
    def clear(self) -> None: ...
    async def wait(self): ...

class ThreadSafeFlag(uio.IOBase):
    _flag: int
    def __init__(self) -> None: ...
    def ioctl(self, req, flags): ...
    def set(self) -> None: ...
    async def wait(self) -> None: ...
