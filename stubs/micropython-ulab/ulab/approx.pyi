def newton(fun: Callable[[float], float], x0: float, *, xtol: float = ..., rtol: float = ..., maxiter: int = ...) -> float: ...
def trapz(y: ulab.array, x: Optional[ulab.array] = ..., dx: float = ...) -> float: ...
